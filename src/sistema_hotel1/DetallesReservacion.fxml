<?import javafx.scene.layout.VBox?>

<VBox styleClass="fondoDetalles" stylesheets="@AHNCSS.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sistema_hotel1.DetallesReservacionController">
   <children>
      <Label alignment="CENTER" maxWidth="1.7976931348623157E308" styleClass="titulo1" text="DETALLES DE RESERVACIONES" textAlignment="CENTER">
