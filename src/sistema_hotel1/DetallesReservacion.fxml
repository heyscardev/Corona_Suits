            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </Label>
      <HBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <children>
            <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="0.0" prefWidth="0.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </HBox>
            <VBox HBox.hgrow="ALWAYS">
               <children>
                  <VBox maxWidth="350.0" spacing="10.0">
                     <children>
                        <Label styleClass="titulo2" text="INFORMACION DEL CLIENTE" />
                        <HBox alignment="TOP_RIGHT">
                           <children>
                              <Label maxWidth="1.7976931348623157E308" text="Cedula Cliente" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="textoCedula" disable="true" />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_RIGHT">
                           <children>
                              <Label maxWidth="1.7976931348623157E308" text="Nombre Cliente" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="textoNombre" disable="true" />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_RIGHT">
                           <children>
                              <Label maxWidth="1.7976931348623157E308" text="Apellido Cliente" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="textoApellido" disable="true" />
