                        </HBox>
                        <HBox alignment="TOP_RIGHT" layoutX="10.0" layoutY="120.0">
                           <children>
                              <Label maxWidth="1.7976931348623157E308" text="Sexo Cliente" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="textoSexo" disable="true" />
                           </children>
                        </HBox>
                        <Label styleClass="titulo2" text="INFORMACION DE LA RESERVACION" />
                        <HBox alignment="TOP_RIGHT">
                           <children>
                              <Label maxWidth="1.7976931348623157E308" text="Codigo Reservacion" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="textoCodigo" disable="true" />
